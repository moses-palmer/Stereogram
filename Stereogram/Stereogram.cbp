<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Stereogram" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/Stereogram" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="--help --in-file zbuffer.png --out-file stereogram.png --pattern-size 75 75" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/Stereogram" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add directory="../arguments" />
			<Add directory="../stereo" />
			<Add directory="./" />
		</Compiler>
		<Linker>
			<Add library="../stereo/libstereo.a" />
			<Add library="pthread" />
			<Add library="libpng12" />
		</Linker>
		<Unit filename="arguments.def" />
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<lib_finder disable_auto="1">
				<lib name="pthread-stubs" />
				<lib name="libpng12" />
			</lib_finder>
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
